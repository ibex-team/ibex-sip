
Variables
    //!--universal="y.*"

x1 in [-1, 1];
x2 in [0, 2];
x3 in [0, 1];
x4 in [-1, 1];
x5 in [0, 2];
x6 in [0, 1];
y1 in [-1,1];
y2 in [-1,1];

minimize  -x3^2-x6^2;

constraints

x2-sqrt((1-x1^2)/0.3)<=0;

(x3^2-(y1-x1)^2-(sqrt((1-y1^2)/0.3)-x2)^2)<=0;

x3 <= x2;

x5-sqrt((1-x4^2)/0.3)<=0;

(x6^2-(y2-x4)^2-(sqrt((1-y2^2)/0.3)-x5)^2)<=0;

x6 <= x5;

(x4-x1)^2 + (x5-x2)^2 >= (x3+x6)^2;

x3 >= x6;

x3 + x6 <= 1.5;

end
